script/source = "extends Node2D

var bpm: float = 98
var seconds_per_beat: float = 60.0 / bpm # seconds per beat - 98bpm
var speed = 300
const block2 = preload(\"res://Objects/My_Stuff/Rhythm/objRhythmBlock2.tscn\")
const slider = preload(\"res://Objects/My_Stuff/Rhythm/objRhythmSlider.tscn\")
var blockSize = 2
var sliderSize = 2
var slider_skew = 0.6

var debug_metronome = preload(\"res://Audio/Sounds/sndCoin.wav\")

enum {UP, DOWN}
var end_beat = 70
var song_finished = false
var score = 0

var notes = [
# first note

[2, 460],
[3, 500],
[4, 540],

[6, 300],
[7, 260],
[8, 220],
[9, 180],
[10, 600],
[11, 700],
[12, 200],
[13, 100],
[14, 400, 16],

# quint into chorus repeat
[17, 300, 18],
[19, 540],
[20, 580],
[21, 320, 22],
[23, 620],
[24, 540],
[25, 500],
[26, 600, 26.5],
[27.5, 300, 28],

[29, 400, 29.5],
[30, 300, 30.5],
[31, 500, 31.5],
[32.5, 150, 33],

# triplets
[34, 200, 34.66],
[35, 400, 35.66],
[36, 600, 36.66],
[37, 750],
[38, 600, 38.66],
[39, 400, 39.66],
[40, 200, 40.66],
[41, 50],
[42, 400, 42.66],
[43, 300, 43.66],
[44, 600, 44.66],
[45, 100],
[46, 700],#last note of triplet section

[48.5,360],
[49, 320],
[49.5, 280],

[50, 240, 50.75],
[51.5, 400],
[54, 180, 54.75],
[55.5, 560],
[56.5, 200],
[57, 350],
[57.5,500],
[58,580, 58.75],
[59.5, 200],
[60.5, 550],
[61,650],
[61.5, 750],
[62,640, 66]
]


var debug_slider_starts_right_consistent = [
	[1,500,2],
	[2.5,0],
	[3,500,5],
	[5.5,0],
	[6,500,10],
]

var debug_slider_starts_left_consistent = [
	[1,300,2],
	[2.5,700],
	[3,300,5],
	[5.5,700],
	[6,300,10],
]

var debug_slider_end_is_prev_x = [
	[1, 500, 2],
	[3, 550, 4],
	[5, 500, 6]
]

# Called when the node enters the scene tree for the first time.
func _ready():
	
	$objRhythmPreviewer.play_sample()
	GLOBAL_MUSIC.finished.connect(on_music_end)
	$Room_related/objPlayer.slide = false
	$Room_related/objPlayer.show_speed = false
	
	var prev_x = 400
	for note in notes:
	#for note in debug_slider_starts_right_consistent:
	#for note in debug_slider_starts_left_consistent:
		if note.size() == 2:
			var n = [note[0], DOWN, note[1]]
			create_note2(n, prev_x)
			prev_x = note[1]
		elif note.size() == 3:
			prev_x = create_slider(note, prev_x)

func _physics_process(_delta):
	$Label.set_text(\"Score: \" + str(score))
	if !song_finished && $Room_related/objPlayer.global_position.y != 295:
		var pos = $Room_related/objPlayer.global_position
		pos.y = 303
		$Room_related/objPlayer.set_global_position(pos)

func create_note2(note, prev_x):
	var note_block = block2.instantiate()
	note_block.speed = speed
	note_block.bpm = bpm
	note_block.beat = note[0]
	note_block.direction = \"up\"
	note_block.x = note[2]
	if note_block.x > prev_x:
		note_block.flip = true
	note_block.collided.connect(score_points)
	$Instances.add_child(note_block)

func create_slider(note, prev_x):
	# this is to make the prev_x based off the end of the slider rather than the start
	var end_offset = (note[2]-note[0]) * speed * seconds_per_beat * slider_skew
	
	var note_block = slider.instantiate()
	note_block.speed = speed
	note_block.bpm = bpm
	note_block.beat = note[0]
	note_block.direction = \"up\"
	note_block.x = note[1] # using raw data instead
	note_block.end_beat = note[2]
	note_block.slider_skew = slider_skew
	note_block.size = sliderSize
	if note_block.x > prev_x:
		note_block.flip = true
		end_offset *= -1
	note_block.collided.connect(score_points)
	$Instances.add_child(note_block)
	
	return note[1] - end_offset
	
func on_music_end():
	$Room_related/objPlayer.v_speed = 400
	$Room_related/objPlayer.h_speed = 3
	$Room_related/objPlayer.slide = true
	song_finished = true
	$Instances.queue_free()
	
func score_points(event):
	if event == \"note\":
		score += 100
	elif event == \"sliderTop\" || event == \"sliderBottom\":
		score += 50
	elif event == \"sliderBody\":
		score += 1
	
	if $scoreReq && score > 5000: 
		$scoreReq.queue_free()
"

