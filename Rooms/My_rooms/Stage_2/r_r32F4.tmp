script/source = "extends Node2D

var bpm: float = 98
var seconds_per_beat: float = 60.0 / bpm # seconds per beat - 98bpm
var speed = 300
const block = preload(\"res://Objects/My_Stuff/Rhythm/objRhythmBlock.tscn\")
const block2 = preload(\"res://Objects/My_Stuff/Rhythm/objRhythmBlock2.tscn\")
var blockSize = 2
var sliderSize = 0.5
var sliderSkew = 16

var debug_metronome = preload(\"res://Audio/Sounds/sndCoin.wav\")

enum {UP, DOWN}

var notes = [
	#[0, DOWN, 100],
	#[1, UP, 100],
#	[2, DOWN, 100],
#	[3, DOWN, 100],
#	[4, DOWN, 100],
#	[5, UP, 100],
#	[9, DOWN, 600]

# first note
[2, 460],
[3, 500],
[4, 540],

[6, 300],
[7, 260],
[8, 220],
[9, 180],
[10, 600],
[11, 700],
[12, 200],
[13, 100],
[14, 400, 16],

# quint into chorus repeat
[17, 300, 18],
[19, 540],
[20, 580],
[21, 320, 22],
[23, 620],
[24, 540],
[25, 500],
[26, 600, 26.5],
[27.5, 300, 28],

[29, 400, 29.5],
[30, 300, 30.5],
[31, 500, 31.5],
[32.5, 200, 33],

# triplets
[34, 400, 34.66],
[35, 500, 35.66],
[36, 600, 36.66],
[37, 700],
[38, 400, 38.66],
[39, 300, 39.66],
[40, 200, 40.66],
[41, 100],
[42, 400, 42.66],
[43, 300, 43.66],
[44, 600, 44.66],
[45, 100],
[46, 700],#last note of triplet section

[48.5,360],
[49, 320],
[49.5, 280],

[50, 240, 50.75],
[51.5, 660],
[54, 180, 54.75],
[55.5, 560],
[56.5, 620],
[57, 660],
[57.5,700],
[58,740, 58.75],
[59.5, 200],
[60.5, 550],
[61,650],
[61.5, 750],
[62,640, 66]
]


# Called when the node enters the scene tree for the first time.
func _ready():
	var prev_x = 400
	for note in notes:
		#create_note2(note)
		if note.size() == 2:
			var n = [note[0], DOWN, note[1]]
			create_note2(n, prev_x)
			prev_x = note[1]
		elif note.size() == 3:
			create_slider(note, prev_x)
		pass
	
	for i in range(100):
		#create_note2([i,UP,100])
		pass
		
	pass # Replace with function body.


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	
	pass

func create_note(note, prev_x):
	#for i in range(3):
		var note_block = block.instantiate()
		add_child(note_block)
		
		var time_to_reach_middle = note[0] * seconds_per_beat
		var distance = time_to_reach_middle * speed
		var y
		if note[1] == UP:
			y = 300 + distance 
			note_block.speed = -speed
		elif note[1] == DOWN:
			y = 300 - distance 
			note_block.speed = speed
		else:
			print(\"error\")
		
		note_block.set_global_position(Vector2(note[2],y))

func create_note2(note, prev_x):
	var note_block = block2.instantiate()
	note_block.speed = speed
	note_block.bpm = bpm
	note_block.beat = note[0]
	note_block.direction = \"up\"
	note_block.x = note[2]
	if note_block.x > prev_x:
		note_block.flip = true
	add_child(note_block)

func create_slider(note, prev_x):
	var note_block = block2.instantiate()
	note_block.speed = speed
	note_block.bpm = bpm
	note_block.beat = note[0]
	note_block.direction = \"up\"
	note_block.x = note[1] # using raw data instead
	if note_block.x > prev_x:
		note_block.flip = true
	
	


func _on_timer_timeout():
	#print(\"beat\")
	GLOBAL_SOUNDS.play_sound(GLOBAL_SOUNDS.sndCoin)
	pass # Replace with function body.
"

